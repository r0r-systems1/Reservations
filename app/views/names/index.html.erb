<h1>Listing Attendees Names</h1>
/views/names/index.html.erb

<table>
  <tr>
    <th>Name</th>
    <th>Eck Id#</th>
    <th>Age</th>
    <th>Seminar</th>
    <th>Hi Conf</th>
    <th>Campout</th>
    <th>Total</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @names.each do |name| %>
  <tr>
    <td><%= name.name %></td>
    <td><%= name.eck_id %></td>
    <td><%= name.age %></td>
    <td><%= name.seminar %></td>
    <td><%= name.hi_conf %></td>
    <td><%= name.YF_campout %></td>
    <td><%= name.total %></td>
    <td><%= link_to 'Show', name %></td>
    <td><%= link_to 'Edit', edit_name_path(name) %></td>
    <td><%= link_to 'Destroy', name, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Name', new_name_path %>
