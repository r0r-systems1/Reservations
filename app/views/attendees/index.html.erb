<h1>Listing attendees</h1>

<table>
  <tr>
    <th>Age</th>
    <th>Gender</th>
    <th>Fname</th>
    <th>Lname</th>
    <th>Eck</th>
    <th>Address</th>
    <th>State</th>
    <th>Country</th>
    <th>Contact</th>
    <th>Designation</th>
    <th>Donation</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @attendees.each do |attendee| %>
  <tr>
    <td><%= attendee.age %></td>
    <td><%= attendee.gender %></td>
    <td><%= attendee.fname %></td>
    <td><%= attendee.lname %></td>
    <td><%= attendee.eck_id %></td>
    <td><%= attendee.address %></td>
    <td><%= attendee.state %></td>
    <td><%= attendee.country %></td>
    <td><%= attendee.contact %></td>
    <td><%= attendee.designation %></td>
    <td><%= attendee.donation %></td>
    <td><%= link_to 'Show', attendee %></td>
    <td><%= link_to 'Edit', edit_attendee_path(attendee) %></td>
    <td><%= link_to 'Destroy', attendee, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Attendee', new_attendee_path %>
